package user

func GetUserEmail() string {
	return "user@example.com"
}

type User struct {
	UserID       int64
	UserName     string
	UserPassword string
	UserEmail    string
	UserPhone    string
}

// GetUserID returns the user id
func (u *User) GetUserID() int64 {
	return u.UserID
}

// GetUserName returns the user name
func (u *User) GetUserName() string {
	return u.UserName
}

func (u *User) GetUserID() int64 {
	return u.UserID
}

func (u *User) GetUserName() string {
	return u.UserName
}

func (u *User) GetUserPassword() string {
	return u.UserPassword
}

func (u *User) GetUserEmail() string {
	return u.UserEmail
}

func (u *User) GetUserPhone() string {
	return u.UserPhone
}
